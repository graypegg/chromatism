{
  "name": "chromatism",
  "version": "2.6.0",
  "description": "A simple set of utility functions for colours.",
  "main": "dist/chromatism.js",
  "module": "dist/chromatism.esm.js",
  "typings": "./index.d.ts",
  "repository": "toish/chromatism",
  "scripts": {
    "test": "BABEL_ENV=test mocha --compilers js:babel-core/register",
    "build": "npm run build:umd && npm run build:es",
    "build:umd": "rollup -c --format umd --output dist/chromatism.js",
    "build:es": "rollup -c --format es --output dist/chromatism.esm.js",
    "format": "eslint --fix src/**/*.js test/**/*.js"
  },
  "keywords": [
    "color",
    "colour",
    "utility",
    "hue",
    "chroma",
    "CMYK",
    "RGB",
    "HSL"
  ],
  "author": "Toish <toish@toi.sh>",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.5.2",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-node": "^5.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^3.2.0",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "uglify-es": "^3.0.13"
  },
  "false": {},
  "browserslist": [
    "ie >= 9",
    "last 2 versions"
  ]
}
